{
    "name": "puresoft/yii2-persian-datepicker",
    "description": "This extension will display a Persian datepicker.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","persian","jalali","date","datepicker","time","calendar"],
    "license": "MIT",
    "homepage": "https://puresoftware.org",
    "authors": [
        {
            "name": "Hesam Gholami",
            "email": "hesamgholami@yahoo.com",
            "homepage": "https://hesam.org"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2",
        "npm-asset/persian-datepicker": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "puresoft\\datepicker\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm"
        }
    }
}
